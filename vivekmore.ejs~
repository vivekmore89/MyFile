<script language="javascript">
        setInterval(blinktext, 500);
        var txt = "";
		var txt1="";
        var count = 0;
		var count1=0;
        function blinktext() {
            var cntrl = document.getElementById("txtblinkingtext");
			var cntrl1 = document.getElementById("txtblinkingtext1");
            if (count == 0)
			{
                txt = cntrl.value;
				txt1 = cntrl1.value;
			}
            if (count % 2 == 0)
			{
                cntrl.value = "";
				cntrl1.value = "";
			}
            else
			{
                cntrl.value = txt;
				cntrl1.value = txt1;
			}
            count++;
			count1++;
        }
    </script>
